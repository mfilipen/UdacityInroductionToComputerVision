%%Matlab code 
%load an image
img=imread('moon.png');


for i=0:
    
    subplot(,3,i*3+1);
    imshow(img);
    
    
    %add salt and pepper
    d=0.1;
    saltPepperImg = imnoise(img,'salt & pepper',d);
    subplot(2,3,i*3+2);
    imshow(saltPepperImg);
    
    
    %apply median filter
    medianFilterNoisyImg = medfilt2(saltPepperImg);
    subplot(2,3,i*3+3);
    imshow(medianFilterNoisyImg);
    
end